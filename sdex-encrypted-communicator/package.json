    "expo-sharing": "~11.2.2",
    "expo-splash-screen": "~0.18.2",
    "expo-sqlite": "~11.1.1",
    "expo-status-bar": "~1.4.4",
    "expo-system-ui": "~2.2.1",
    "expo-updates": "~0.16.4",
    "js-crypto-aes": "^1.0.4",
    "keypair": "^1.0.4",
    "nativewind": "^2.0.11",
