    "@react-navigation/stack": "^6.3.12",
    "@tsconfig/react-native": "^2.0.3",
    "@types/crypto-js": "^4.1.1",
    "@types/deepmerge": "^2.2.0",
    "@types/react": "~18.0.27",
    "@types/react-dom": "~18.0.10",
    "@types/react-native": "~0.71.5",
    "@types/react-native-vector-icons": "^6.4.13",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.29.3",
