{
  "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-tailwindcss"],
  "pluginSearchDirs": [],
  "tailwindConfig": "./tailwind.config.js",
  "quoteProps": "as-needed",
  "singleQuote": false,
  "printWidth": 100,
  "editor.formatOnSave": true,
  "proseWrap": "always",
  "tabWidth": 2,
  "requireConfig": false,
  "useTabs": false,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "semi": true,
  "arrowParens": "always",
  "jsxSingleQuote": false,
  "trailingComma": "all"
}
