{
  "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-tailwindcss"],
  "pluginSearchDirs": [],
  "tailwindConfig": "./tailwind.config.js",
  "quoteProps": "as-needed",
  "singleQuote": false,
  "printWidth": 100,
  "proseWrap": "always",
  "tabWidth": 2,
  "endOfLine": "lf",
  "useTabs": false,
  "bracketSpacing": true,
  "semi": true,
  "arrowParens": "always",
  "jsxSingleQuote": false,
  "trailingComma": "all",
  "overrides": [
    {
      "files": ".prettierrc",
      "options": { "parser": "json" }
    }
  ]
}
